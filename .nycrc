{
    "check-coverage": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "watermarks": {
          "lines": [80, 95],
          "functions": [80, 95],
          "branches": [80, 95],
          "statements": [80, 95]
        },
    "include": [
      "app.js",
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./reports/"

}
